    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "private": true,
  "dependencies": {
    "commander": "^14.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xvantz/rage-console.git"
  },
  "keywords": [
    "rage-mp",
    "logger",
    "cef",
    "client",
    "server",
    "typescript"
