{
  "name": "@xvantz/rage-console",
  "version": "0.1.0",
  "description": "Единый логгер для RAGE MP server/client и CEF",
  "author": "xvantz",
  "license": "Elastic License 2.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "ui-static",
    "README.md",
