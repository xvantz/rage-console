{
  "compilerOptions": {
	"baseUrl": ".",
	"target": "ES2020",
	"lib": ["ES2020", "DOM"],
	"module": "ESNext",
	"moduleResolution": "Bundler",
	"strict": true,
	"declaration": true,
	"declarationMap": true,
	"skipLibCheck": true,
	"esModuleInterop": true,
	"resolveJsonModule": true,
	"isolatedModules": true,
	"outDir": "dist",
	"types": ["node", "@ragempcommunity/types-client", "@ragempcommunity/types-cef", "@ragempcommunity/types-server"],
	"paths": {
	  "@types/*": ["src/types.ts"]
	}
  },
  "include": ["src"]
}
